generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Entry{
  id String @id @default(uuid())
  title String
  content String
  createdAt DateTime @default(now())
  skill Skill[]
}

enum Skill {
  AI
  BUG_FIXES
  CSS
  DATABASES
  GIT
  EXTERNAL_LIBRARIES
  HTML
  JAVASCRIPT
  JEST
  LEGACY_CODE
  MISC
  NEXT_JS
  NODE_JS
  NPM
  REACT
  REST_APIS
  SASS
  SQL
  TYPESCRIPT
}